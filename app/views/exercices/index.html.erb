<div>
  <h1>Exercices#index</h1>
  <div class="card-product">
    <div class="card-product-infos">
      <% @exercices.each do |exercice| %>
        <h2>Title : <%= exercice.title%></h2>
        <p>Details : <%= exercice.details%></p>
        <p>Instruction : <%= exercice.instruction%> </p>
        <p>Exp : <%= exercice.exp%> </p>
      <% end %>
  </div>
</div>
